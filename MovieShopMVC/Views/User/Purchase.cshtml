@model List<ApplicationCore.Models.MovieCardModel>

<h3>Movies You Have Purchased :</h3>
<hr />
<div class="me-2">
    <div class="row">
        @foreach (var movieCard in Model)
        {
        <div class="col-xl-2 col-lg-3 col-sm-4 col-6">
            @*call the partial view MovieCard*@
            <partial name="_MovieCard" model="movieCard"></partial>
        </div>
            
        }

    </div>
</div>
